{
  "name": "vue-ele-ui",
  "version": " 1.2.2",
  "private": false,
  "license": "ISC",
  "author": "hanshao",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "package": "vue-cli-service build --target lib ./packages/index.js --name vue-ele-ui --dest vue-ele-ui",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue-ele-ui --dest lib packages/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "publish": "git push && git push --tags && npm publish",
    "deploy": "bash deploy.sh"
  },
  "main": "lib/vue-ele-ui.common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dawn-ferry/vue-ele-ui"
  },
  "homepage": "https://dawn-ferry.github.io/vue-ele-ui/",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "keywords": [
    "elementUI",
    "vue",
    "components",
    "table"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.15.8",
    "vue": "^2.6.11",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass": "^1.51.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}